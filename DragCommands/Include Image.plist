<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>command</key>
	<string>if [[ -z $TM_DIRECTORY ]]
	then filename=$TM_DROPPED_FILE
	else 
		filename=${TM_DROPPED_FILE#${TM_DIRECTORY}}
fi
echo "\\\\begin{figure}[\${1:htbp}]
	\\\\begin{center}
		\\\\includegraphics[height=\${2:3in}]{$filename}
	\\\\end{center}
	\\\\caption{\${4:caption}}
	\\\\label{fig:\${5:label}}
\\\\end{figure}"</string>
	<key>documentExtensions</key>
	<array>
		<string>html</string>
		<string>htm</string>
		<string>rhtml</string>
		<string>shtml</string>
		<string>phtml</string>
		<string>php</string>
		<string>php3</string>
		<string>php4</string>
		<string>php5</string>
		<string>inc</string>
		<string>cfm</string>
		<string>cfml</string>
		<string>dbm</string>
		<string>dbml</string>
	</array>
	<key>draggedFileExtensions</key>
	<array>
		<string>jpg</string>
		<string>jpeg</string>
		<string>pdf</string>
		<string>eps</string>
	</array>
	<key>name</key>
	<string>Include Image</string>
	<key>output</key>
	<string>insertAsSnippet</string>
	<key>scope</key>
	<string>text.latex</string>
	<key>uuid</key>
	<string>2C5B80BC-C9C8-11D9-B88C-000A95C0F626</string>
</dict>
</plist>
